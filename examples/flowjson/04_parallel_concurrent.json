{
  "start": "p",
  "nodes": {
    "p": {
      "kind": "parallel",
      "parallel_services": ["transform", "transform"],
      "prep": { "input_key": "$params.val" },
      "params": { "mul": 3.0 },
      "post": { "output_key": "agg", "action_static": "next" },
      "parallel_mode": "concurrent",
      "max_parallel": 2
    },
    "end": { "kind": "executor", "service": "sum", "prep": { "input_key": "$shared.agg" }, "post": { "output_key": "total" } }
  },
  "edges": [
    { "from": "p", "action": "next", "to": "end" }
  ]
}
