{
  "start": "timer",
  "nodes": {
    "timer": { "kind": "timer", "params": { "delay_ms": 200 }, "post": { "action_static": "go" } },
    "wait": { "kind": "wait_event", "params": { "signal_key": "$shared.flag", "timeout_ms": 500 }, "post": { "action_static": "go" } },
    "end": { "kind": "executor", "service": "transform", "prep": { "input_key": "$params.val" }, "params": { "mul": 1.0 }, "post": { "output_key": "result" } }
  },
  "edges": [
    { "from": "timer", "action": "go", "to": "wait" },
    { "from": "wait", "action": "go", "to": "end" }
  ]
}
