{
  "start": "parallel_mix",
  "nodes": {
    "parallel_mix": {
      "kind": "parallel",
      "parallel_mode": "concurrent",
      "parallel_services": [
        "sync_branch",
        "async-transform"
      ],
      "parallel_execs": [
        {
          "service": "sync_branch",
          "exec_type": "local_func",
          "func": "upper",
          "prep": {
            "input_key": "$params.text"
          },
          "post": {
            "output_key": "local_out"
          }
        },
        {
          "service": "async-transform",
          "exec_type": "queue",
          "prep": {
            "input_key": "$params.text"
          },
          "params": {
            "text": "async-processing"
          },
          "post": {
            "output_key": "async_out"
          }
        }
      ],
      "post": {
        "output_key": "parallel_results",
        "action_static": "end"
      }
    },
    "end": {
      "kind": "executor",
      "exec_type": "local_func",
      "func": "log_result",
      "params": {}
    }
  },
  "edges": [
    {
      "from": "parallel_mix",
      "action": "end",
      "to": "end"
    },
    {
      "from": "end",
      "action": "default",
      "to": ""
    }
  ]
}