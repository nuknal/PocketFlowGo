{
  "start": "choice",
  "nodes": {
    "choice": {
      "kind": "choice",
      "choice_cases": [
        { "action": "B", "expr": { "eq": ["$params.path", "B"] } },
        { "action": "C", "expr": { "eq": ["$params.path", "C"] } }
      ],
      "default_action": "B"
    },
    "B": { "kind": "executor", "service": "transform", "prep": { "input_key": "$params.val" }, "params": { "mul": 10.0 }, "post": { "output_key": "result_b" } },
    "C": { "kind": "executor", "service": "transform", "prep": { "input_key": "$params.val" }, "params": { "mul": 20.0 }, "post": { "output_key": "result_c" } }
  },
  "edges": [
    { "from": "choice", "action": "B", "to": "B" },
    { "from": "choice", "action": "C", "to": "C" }
  ]
}
